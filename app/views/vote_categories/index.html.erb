<p id="notice"><%= notice %></p>

<h1>Vote Categories</h1>

<table>
  <thead>
    <tr>
      <th>Vote</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vote_categories.each do |vote_category| %>
      <tr>
        <td><%= vote_category.vote_id %></td>
        <td><%= vote_category.category_id %></td>
        <td><%= link_to 'Show', vote_category %></td>
        <td><%= link_to 'Edit', edit_vote_category_path(vote_category) %></td>
        <td><%= link_to 'Destroy', vote_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote Category', new_vote_category_path %>
